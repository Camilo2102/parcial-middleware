type Query {
    findEmployeeById(id: ID): Employee
    findDepartmentById(id: ID): Department
    findEmployees: [Employee]
}

type Employee{
    id: ID,
    name: String,
    lastName: String,
    role: String
}

type Department {
    id: ID,
    departmentName: String,
    description: String,
    employees: [Employee]
}